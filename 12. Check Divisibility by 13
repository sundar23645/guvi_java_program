import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
       
        Scanner obj = new Scanner(System.in);
        int n = obj.nextInt();
        int f=0;
        for(int i=1;i<=n;i++)
        {
            if(n%13==0)
            {
                f=1;
            }
        }
        if(f==1)
        {
            System.out.print("yes");
        }else{
            System.out.print("no");
        }
    }
}