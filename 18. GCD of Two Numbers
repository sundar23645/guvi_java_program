import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        //int c=a;
        int d=b;
        
        while(b!=0){
            int temp=b;
            b=a%b;
            a=temp;
        }
        if(d==0)
        {
            System.out.println("-1");
        }else{
            System.out.println(a);
        }
        
    }
}