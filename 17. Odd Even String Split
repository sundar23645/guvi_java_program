import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        //A Simple Hello World
       Scanner sc=new Scanner(System.in);
       String a=sc.next();
       String odd="";
       String even="";
       
       for(int i=0;i<a.length();i++)
       {
           char ch=a.charAt(i);
           if((i+1)%2!=0)
           {
               odd=odd+ch;
           }else{
               even=even+ch;
           }
       }
       System.out.print(odd+" "+even);
    }
}