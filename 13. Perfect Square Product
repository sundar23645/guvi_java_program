import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
       
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int prod=a*b;
       double sqr=Math.sqrt(prod);
       if(prod==(int)sqr*(int)sqr)
       {
           System.out.println("yes");
       }
       else{
           System.out.println("no");
       }
    }
}