import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
       
       Scanner sc=new Scanner(System.in);
       int t=sc.nextInt();
       int c=0;
       int a = 0;
       boolean f=true;
       while (t > 0) {
            a = a * 10 + t % 10;
            t /= 10;
       }
       while(a!=0)
       {
           
           int rem=a%10;
           if(rem%2!=0)
           {
               c++;
               if(!f){
                    System.out.print(" ");
               }
               System.out.print(rem);
               f=false;
           }
           a=a/10;
       }
       if(c==0)
       {
           System.out.print("-1");
       }
       
    }
}