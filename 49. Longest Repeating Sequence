import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       int arr[]=new int[n];
       for(int i=0;i<n;i++)
       {
           arr[i]=sc.nextInt();
       }
       int res=0;
       for(int i=0;i<n;i++)
       {
           int c=0;
           for(int j=i;j<n;j++)
           {
               if(arr[i]==arr[j])
               {
                     c++;
                                           
               }
               if(c>res){
               res=c;
               }
           }
           
       }
       if(res>1)
        System.out.print(res);
       else 
        System.out.print("-1");
    }
}